{"ast":null,"code":"import { ref, onMounted } from 'vue';\nimport StoryModal from '@/components/StoryModal.vue'; // Import the StoryModal component\n\nexport default {\n  name: 'HomePage',\n  components: {\n    StoryModal // Register StoryModal as a component\n  },\n  setup() {\n    const showStory = ref(false);\n    const openStory = () => {\n      showStory.value = true;\n      document.body.style.overflow = 'hidden';\n    };\n    const closeStory = () => {\n      showStory.value = false;\n      document.body.style.overflow = 'auto';\n    };\n    const scrollToContent = () => {\n      const nextSection = document.querySelector('.services');\n      if (nextSection) {\n        nextSection.scrollIntoView({\n          behavior: 'smooth'\n        });\n      }\n    };\n    const handleParallax = () => {\n      const layers = document.querySelectorAll('.parallax-layer');\n      window.addEventListener('scroll', () => {\n        const scrollPosition = window.pageYOffset;\n        layers.forEach(layer => {\n          const speed = layer.getAttribute('data-speed');\n          layer.style.transform = `translateY(${scrollPosition * speed}px)`;\n        });\n      });\n    };\n    onMounted(() => {\n      handleParallax();\n    });\n    return {\n      showStory,\n      openStory,\n      closeStory,\n      scrollToContent\n    };\n  }\n};","map":{"version":3,"names":["ref","onMounted","StoryModal","name","components","setup","showStory","openStory","value","document","body","style","overflow","closeStory","scrollToContent","nextSection","querySelector","scrollIntoView","behavior","handleParallax","layers","querySelectorAll","window","addEventListener","scrollPosition","pageYOffset","forEach","layer","speed","getAttribute","transform"],"sources":["src/views/HomePage.vue"],"sourcesContent":["<template>\n  <div id=\"home\">\n    <!-- Hero Section -->\n    <section class=\"hero\">\n      <div class=\"hero-background\">\n        <!-- Parallax Background Layers -->\n        <div class=\"parallax-layer layer-1\"></div>\n        <div class=\"parallax-layer layer-2\"></div>\n        <div class=\"parallax-layer layer-3\"></div>\n        <div class=\"background-overlay\"></div>\n        <div class=\"color-overlay\"></div>\n      </div>\n      <div class=\"container hero-container\">\n        <div class=\"hero-content\">\n          <h1 class=\"hero-title\">Discover Your True Potential</h1>\n          <p class=\"hero-subtitle\">\n            Join me, Dawlat Emad, on a transformative journey towards self-discovery and personal growth.\n          </p>\n          <button class=\"hero-button\" @click=\"openStory\">My Story</button>\n        </div>\n        <div class=\"hero-image-wrapper\">\n          <img\n            src=\"@/assets/images/dawlat.png\"\n            alt=\"Dawlat Emad\"\n            class=\"hero-image\"\n          />\n        </div>\n      </div>\n      <div class=\"scroll-indicator\" @click=\"scrollToContent\">\n        <span></span>\n      </div>\n    </section>\n\n    <!-- Use the StoryModal component here -->\n    <StoryModal :showStory=\"showStory\" @close=\"closeStory\" />\n  </div>\n</template>\n\n<script>\nimport { ref, onMounted } from 'vue';\nimport StoryModal from '@/components/StoryModal.vue'; // Import the StoryModal component\n\nexport default {\n  name: 'HomePage',\n  components: {\n    StoryModal, // Register StoryModal as a component\n  },\n  setup() {\n    const showStory = ref(false);\n\n    const openStory = () => {\n      showStory.value = true;\n      document.body.style.overflow = 'hidden';\n    };\n\n    const closeStory = () => {\n      showStory.value = false;\n      document.body.style.overflow = 'auto';\n    };\n\n    const scrollToContent = () => {\n      const nextSection = document.querySelector('.services');\n      if (nextSection) {\n        nextSection.scrollIntoView({ behavior: 'smooth' });\n      }\n    };\n\n    const handleParallax = () => {\n      const layers = document.querySelectorAll('.parallax-layer');\n      window.addEventListener('scroll', () => {\n        const scrollPosition = window.pageYOffset;\n        layers.forEach((layer) => {\n          const speed = layer.getAttribute('data-speed');\n          layer.style.transform = `translateY(${scrollPosition * speed}px)`;\n        });\n      });\n    };\n\n    onMounted(() => {\n      handleParallax();\n    });\n\n    return {\n      showStory,\n      openStory,\n      closeStory,\n      scrollToContent,\n    };\n  },\n};\n</script>\n"],"mappings":"AAuCA,SAAAA,GAAA,EAAAC,SAAA;AACA,OAAAC,UAAA;;AAEA;EACAC,IAAA;EACAC,UAAA;IACAF,UAAA;EACA;EACAG,MAAA;IACA,MAAAC,SAAA,GAAAN,GAAA;IAEA,MAAAO,SAAA,GAAAA,CAAA;MACAD,SAAA,CAAAE,KAAA;MACAC,QAAA,CAAAC,IAAA,CAAAC,KAAA,CAAAC,QAAA;IACA;IAEA,MAAAC,UAAA,GAAAA,CAAA;MACAP,SAAA,CAAAE,KAAA;MACAC,QAAA,CAAAC,IAAA,CAAAC,KAAA,CAAAC,QAAA;IACA;IAEA,MAAAE,eAAA,GAAAA,CAAA;MACA,MAAAC,WAAA,GAAAN,QAAA,CAAAO,aAAA;MACA,IAAAD,WAAA;QACAA,WAAA,CAAAE,cAAA;UAAAC,QAAA;QAAA;MACA;IACA;IAEA,MAAAC,cAAA,GAAAA,CAAA;MACA,MAAAC,MAAA,GAAAX,QAAA,CAAAY,gBAAA;MACAC,MAAA,CAAAC,gBAAA;QACA,MAAAC,cAAA,GAAAF,MAAA,CAAAG,WAAA;QACAL,MAAA,CAAAM,OAAA,CAAAC,KAAA;UACA,MAAAC,KAAA,GAAAD,KAAA,CAAAE,YAAA;UACAF,KAAA,CAAAhB,KAAA,CAAAmB,SAAA,iBAAAN,cAAA,GAAAI,KAAA;QACA;MACA;IACA;IAEA3B,SAAA;MACAkB,cAAA;IACA;IAEA;MACAb,SAAA;MACAC,SAAA;MACAM,UAAA;MACAC;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}