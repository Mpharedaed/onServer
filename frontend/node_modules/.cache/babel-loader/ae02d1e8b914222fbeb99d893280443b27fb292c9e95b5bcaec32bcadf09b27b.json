{"ast":null,"code":"import axios from 'axios';\nimport { mapGetters } from 'vuex';\nexport default {\n  name: 'UserProfile',\n  data() {\n    return {\n      user: {\n        name: this.$store.state.auth.user.name\n      },\n      posts: []\n    };\n  },\n  computed: {\n    ...mapGetters(['isAuthenticated', 'authToken'])\n  },\n  methods: {\n    async fetchUserPosts() {\n      try {\n        const response = await axios.get('http://127.0.0.1:5000/api/user/posts', {\n          headers: {\n            Authorization: `Bearer ${this.authToken}`\n          }\n        });\n        this.posts = response.data.posts;\n      } catch (error) {\n        console.error('Error fetching posts:', error);\n      }\n    }\n  },\n  created() {\n    if (this.isAuthenticated) {\n      this.fetchUserPosts();\n    }\n  }\n};","map":{"version":3,"names":["axios","mapGetters","name","data","user","$store","state","auth","posts","computed","methods","fetchUserPosts","response","get","headers","Authorization","authToken","error","console","created","isAuthenticated"],"sources":["src/components/UserProfile.vue"],"sourcesContent":["<template>\r\n  <div class=\"user-profile\">\r\n    <h2>{{ user.name }}'s Profile</h2>\r\n    <div v-if=\"posts.length\">\r\n      <h3>Your Posts</h3>\r\n      <ul>\r\n        <li v-for=\"post in posts\" :key=\"post.id\">\r\n          <h4>{{ post.content }}</h4>\r\n          <p>{{ post.created_at }}</p>\r\n        </li>\r\n      </ul>\r\n    </div>\r\n    <div v-else>\r\n      <p>You have no posts.</p>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\nimport { mapGetters } from 'vuex';\r\n\r\nexport default {\r\n  name: 'UserProfile',\r\n  data() {\r\n    return {\r\n      user: {\r\n        name: this.$store.state.auth.user.name\r\n      },\r\n      posts: []\r\n    };\r\n  },\r\n  computed: {\r\n    ...mapGetters(['isAuthenticated', 'authToken'])\r\n  },\r\n  methods: {\r\n    async fetchUserPosts() {\r\n      try {\r\n        const response = await axios.get('http://127.0.0.1:5000/api/user/posts', {\r\n          headers: {\r\n            Authorization: `Bearer ${this.authToken}`\r\n          }\r\n        });\r\n        this.posts = response.data.posts;\r\n      } catch (error) {\r\n        console.error('Error fetching posts:', error);\r\n      }\r\n    }\r\n  },\r\n  created() {\r\n    if (this.isAuthenticated) {\r\n      this.fetchUserPosts();\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.user-profile {\r\n  max-width: 600px;\r\n  margin: 0 auto;\r\n  padding: 20px;\r\n}\r\n</style>\r\n"],"mappings":"AAmBA,OAAAA,KAAA;AACA,SAAAC,UAAA;AAEA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,IAAA;QACAF,IAAA,OAAAG,MAAA,CAAAC,KAAA,CAAAC,IAAA,CAAAH,IAAA,CAAAF;MACA;MACAM,KAAA;IACA;EACA;EACAC,QAAA;IACA,GAAAR,UAAA;EACA;EACAS,OAAA;IACA,MAAAC,eAAA;MACA;QACA,MAAAC,QAAA,SAAAZ,KAAA,CAAAa,GAAA;UACAC,OAAA;YACAC,aAAA,iBAAAC,SAAA;UACA;QACA;QACA,KAAAR,KAAA,GAAAI,QAAA,CAAAT,IAAA,CAAAK,KAAA;MACA,SAAAS,KAAA;QACAC,OAAA,CAAAD,KAAA,0BAAAA,KAAA;MACA;IACA;EACA;EACAE,QAAA;IACA,SAAAC,eAAA;MACA,KAAAT,cAAA;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}