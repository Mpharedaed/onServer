{"ast":null,"code":"import { mapGetters, mapActions } from 'vuex';\nimport logoImage from '@/assets/logo.png'; // Ensure this path is correct\n\nexport default {\n  name: 'AppHeader',\n  data() {\n    return {\n      menuOpen: false,\n      isScrolled: false,\n      logoImage\n    };\n  },\n  computed: {\n    ...mapGetters(['isAuthenticated'])\n  },\n  methods: {\n    ...mapActions(['logout']),\n    logoutUser() {\n      this.logout().then(() => {\n        this.$router.push('/login');\n      });\n    },\n    toggleMenu() {\n      this.menuOpen = !this.menuOpen;\n    },\n    closeMenu() {\n      this.menuOpen = false;\n    },\n    handleScroll() {\n      this.isScrolled = window.scrollY > 50;\n    }\n  },\n  mounted() {\n    window.addEventListener('scroll', this.handleScroll);\n  },\n  beforeUnmount() {\n    window.removeEventListener('scroll', this.handleScroll);\n  }\n};","map":{"version":3,"names":["mapGetters","mapActions","logoImage","name","data","menuOpen","isScrolled","computed","methods","logoutUser","logout","then","$router","push","toggleMenu","closeMenu","handleScroll","window","scrollY","mounted","addEventListener","beforeUnmount","removeEventListener"],"sources":["src/components/AppHeader.vue"],"sourcesContent":["<template>\n  <header class=\"header\" :class=\"{ scrolled: isScrolled }\">\n    <div class=\"container\">\n      <div class=\"logo\">\n        <router-link to=\"/\">\n          <img :src=\"logoImage\" alt=\"Trillow Logo\" />\n        </router-link>\n      </div>\n      <nav class=\"navigation\" :class=\"{ open: menuOpen }\">\n        <ul>\n          <li><router-link to=\"/\">Home</router-link></li>\n          <li><router-link to=\"/about\">About</router-link></li>\n          <li v-if=\"isAuthenticated\"><router-link to=\"/dashboard\">Dashboard</router-link></li>\n          <li v-if=\"isAuthenticated\"><router-link to=\"/profile\">Profile</router-link></li>\n          <li v-if=\"!isAuthenticated\"><router-link to=\"/login\">Login</router-link>