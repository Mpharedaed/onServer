menuOpen = false;\n    },\n    handleScroll() {\n      this.isScrolled = window.scrollY > 50;\n    },\n  },\n  mounted() {\n    window.addEventListener('scroll', this.handleScroll);\n  },\n  beforeUnmount() {\n    window.removeEventListener('scroll', this.handleScroll);\n  },\n};\n</script>\n"],"mappings":"AA0CA,SAAAA,UAAA,EAAAC,UAAA;AACA,OAAAC,SAAA;;AAEA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,QAAA;MACAC,UAAA;MACAJ;IACA;EACA;EACAK,QAAA;IACA,GAAAP,UAAA;EACA;EACAQ,OAAA;IACA,GAAAP,UAAA;IACAQ,WAAA;MACA,KAAAC,MAAA,GAAAC,IAAA;QACA,KAAAC,OAAA,CAAAC,IAAA;MACA;IACA;IACAC,WAAA;MACA,KAAAT,QAAA,SAAAA,QAAA;IACA;IACAU,UAAA;MACA,KAAAV,QAAA;IACA;IACAW,aAAA;MACA,KAAAV,UAAA,GAAAW,MAAA,CAAAC,OAAA;IACA;EACA;EACAC,QAAA;IACAF,MAAA,CAAAG,gBAAA,gBAAAJ,YAAA;EACA;EACAK,cAAA;IACAJ,MAAA,CAAAK,mBAAA,gBAAAN,YAAA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}