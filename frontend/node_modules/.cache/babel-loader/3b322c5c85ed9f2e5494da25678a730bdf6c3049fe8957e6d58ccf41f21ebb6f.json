{"ast":null,"code":"import axios from 'axios';\nexport default {\n  name: 'Dashboard',\n  data() {\n    return {\n      username: '',\n      message: ''\n    };\n  },\n  created() {\n    this.fetchDashboardData();\n  },\n  methods: {\n    async fetchDashboardData() {\n      try {\n        const response = await axios.get('http://127.0.0.1:5000/api/dashboard', {\n          headers: {\n            'x-access-token': localStorage.getItem('token')\n          }\n        });\n        this.username = response.data.message.split(' ')[1]; // Extract username from message\n        this.message = response.data.data;\n      } catch (error) {\n        console.error('Error fetching dashboard data:', error);\n        this.$router.push('/login'); // Redirect to login if not authenticated\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","username","message","created","fetchDashboardData","methods","response","get","headers","localStorage","getItem","split","error","console","$router","push"],"sources":["src/views/Dashboard.vue"],"sourcesContent":["<template>\r\n  <div class=\"dashboard\">\r\n    <h1>Dashboard</h1>\r\n    <p>Welcome, {{ username }}!</p>\r\n    <p>{{ message }}</p>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  name: 'Dashboard',\r\n  data() {\r\n    return {\r\n      username: '',\r\n      message: ''\r\n    };\r\n  },\r\n  created() {\r\n    this.fetchDashboardData();\r\n  },\r\n  methods: {\r\n    async fetchDashboardData() {\r\n      try {\r\n        const response = await axios.get('http://127.0.0.1:5000/api/dashboard', {\r\n          headers: { 'x-access-token': localStorage.getItem('token') }\r\n        });\r\n        this.username = response.data.message.split(' ')[1]; // Extract username from message\r\n        this.message = response.data.data;\r\n      } catch (error) {\r\n        console.error('Error fetching dashboard data:', error);\r\n        this.$router.push('/login'); // Redirect to login if not authenticated\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.dashboard {\r\n  padding: 20px;\r\n}\r\n</style>\r\n"],"mappings":"AASA,OAAAA,KAAA;AAEA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,QAAA;MACAC,OAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,kBAAA;EACA;EACAC,OAAA;IACA,MAAAD,mBAAA;MACA;QACA,MAAAE,QAAA,SAAAR,KAAA,CAAAS,GAAA;UACAC,OAAA;YAAA,kBAAAC,YAAA,CAAAC,OAAA;UAAA;QACA;QACA,KAAAT,QAAA,GAAAK,QAAA,CAAAN,IAAA,CAAAE,OAAA,CAAAS,KAAA;QACA,KAAAT,OAAA,GAAAI,QAAA,CAAAN,IAAA,CAAAA,IAAA;MACA,SAAAY,KAAA;QACAC,OAAA,CAAAD,KAAA,mCAAAA,KAAA;QACA,KAAAE,OAAA,CAAAC,IAAA;MACA;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}